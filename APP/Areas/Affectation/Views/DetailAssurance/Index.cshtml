@model APP.Areas.Affectation.Models.AllViewsModel
@using System.Diagnostics

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_2.cshtml";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-offset-4 col-lg-4">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Assurer un véhicule</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    @using (Html.BeginForm("Ajouter", "DetailAssurance", new { area = "Affectation" }, FormMethod.Post, new { @class = "form-horizontal", @role = "form", @id = "form" }))
                    {
                        <div class="form-horizontal">
                            <div class="form-group">
                                <input type="hidden" id="Id" name="Id" placeholder="ID" class="form-control">
                            </div>
                            <div class="form-group">
                                <label class="col-sm-4 control-label">Assurance </label>
                                <div class="col-sm-8">
                                    <select name="IdAssurance" class="select2_demo_3 form-control">
                                        <option> </option>
                                        @{
                                            foreach (var veh in Model.assurance)
                                            {
                                                if (Model.DetAss != null)
                                                {
                                                    if (veh.IdAssurance == Model.DetAss.IdAssurance)
                                                    {
                                                        <option selected value="@veh.IdAssurance">@veh.LibelleAssurance </option>
                                                    }
                                                    else
                                                    {
                                                        <option value="@veh.IdAssurance">@veh.LibelleAssurance </option>
                                                    }
                                                }
                                                else
                                                {
                                                    <option value="@veh.IdAssurance">@veh.LibelleAssurance </option>
                                                }
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-4 control-label">Véhicule</label>
                                <div class="col-sm-8">
                                    <select name="IdVehicule" class="select2_demo_3 form-control">
                                        <option> </option>
                                        @{
                                            foreach (var veh in Model.Vehicule)
                                            {
                                                if (Model.DetAss != null)
                                                {
                                                    if (veh.IdVehicule == Model.DetAss.IdVehicule)
                                                    {
                                                        <option selected value="@veh.IdVehicule">@veh.MarqueVehicule.LibelleMarque @veh.NomVehicule @veh.Immatriculation</option>
                                                    }
                                                    else
                                                    {
                                                        <option value="@veh.IdVehicule">@veh.MarqueVehicule.LibelleMarque @veh.NomVehicule @veh.Immatriculation</option>
                                                    }
                                                }
                                                else
                                                {
                                                    <option value="@veh.IdVehicule">@veh.MarqueVehicule.LibelleMarque @veh.NomVehicule @veh.Immatriculation</option>
                                                }
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                            <div class="form-group" id="data_1">
                                <label class="col-sm-4 control-label">Début de couverture</label>
                                <div class="col-xs-8">
                                    <input type="date" class="form-control" value="@Convert.ToDateTime(Model.DetAss.DataDebutCouverture).ToString("yyyy-MM-dd")" name="DataDebutCouverture">
                                </div>
                            </div>
                            <div class="form-group" id="data_1">
                                <label class="col-sm-4 control-label">Fin de couverture</label>
                                <div class="col-xs-8">
                                    <input type="date" class="form-control" value="@Convert.ToDateTime(Model.DetAss.DateFinCouverture).ToString("yyyy-MM-dd")" name="DateFinCouverture">
                                </div>
                            </div>
                            @if (ViewData["CompareDate"] != null)
                            {
                                <div class="pull-right">
                                    <div class="alert alert-danger alert-dismissable">
                                        <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
                                        @ViewData["CompareDate"]
                                    </div>
                                </div>
                            }
                        </div>
                        <div class="form-group">
                            <div class="col-sm-8 col-sm-offset-4">
                                <button class="btn btn-white" type="reset">Annuler </button>
                                <button class="btn btn-primary" type="submit">Ajouter </button>
                            </div>
                        </div>
                                            }
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-offset-2 col-lg-8">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <a class="btn btn-primary btn-sm pull-right" href="@Url.Action("ExportListC", "DetailAssurance")" target="_blank">
                        Imprimer
                    </a>
                    <h2>Liste des Véhicules avec leurs assurances </h2>
                </div>
                <div class="ibox-content">
                    <div class="col-lg-offset-7 col-lg-5">
                        <input type="text" class="form-control input-sm m-b-xs" id="filter"
                               placeholder="Rechercher">
                    </div>
                    <table class="footable table table-stripped" data-page-size="8" data-filter=#filter>
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayName("ID")
                                </th>
                                <th>
                                    @Html.DisplayName("Assurance")
                                </th>
                                <th>
                                    @Html.DisplayName("Véhicule")
                                </th>
                                <th>
                                    @Html.DisplayName("Période de couverture")
                                </th>
                                <th class="text-right">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.detailAssurance.OrderByDescending(x => x.IdDetailAssurance))
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.IdDetailAssurance)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Assurance.LibelleAssurance)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Vehicule.MarqueVehicule.LibelleMarque) @Html.DisplayFor(modelItem => item.Vehicule.NomVehicule)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.DataDebutCouverture) - @Html.DisplayFor(modelItem => item.DateFinCouverture)
                                    </td>
                                    <td class="text-right">
                                        <div class="btn-group">
                                            <a class="btn btn-white btn-bitbucket" data-toggle="modal" data-target="#e-@Html.DisplayFor(modelItem => item.IdDetailAssurance)">
                                                Modifier
                                            </a>
                                            <a class="btn btn-white btn-bitbucket" data-toggle="modal" data-target="#dl-@Html.DisplayFor(modelItem => item.IdDetailAssurance)">
                                                Détail
                                            </a>
                                            <a class="btn btn-white btn-bitbucket" data-toggle="modal" data-target="#de-@Html.DisplayFor(modelItem => item.IdDetailAssurance)">
                                                Mettre Fin
                                            </a>
                                            @*@{
                                                    if (@item.Supprimer == false)
                                                    {
                                                        <a class="btn btn-white btn-bitbucket" href="/Affectation/CategorieMissions/Etat/@item.IdMarque">
                                                            Supprimer
                                                        </a>
                                                    }
                                                    else
                                                    {
                                                        <a class="btn btn-white btn-bitbucket" href="/Affectation/CategorieMissions/Etat/@item.IdMarque">
                                                            Restaurer
                                                        </a>
                                                    }
                                                }*@
                                        </div>
                                    </td>
                                </tr>
                            }
                        </tbody>
                        <tfoot>
                            <tr>
                                <td colspan="10">
                                    <ul class="pagination pull-right"></ul>
                                </td>
                            </tr>
                        </tfoot>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

