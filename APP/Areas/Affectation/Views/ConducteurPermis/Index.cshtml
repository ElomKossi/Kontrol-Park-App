@model APP.Areas.Affectation.Models.AllViewsModel
@using DAL;
@using System.Diagnostics

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_2.cshtml";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-offset-3 col-lg-6">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>
                        Ajouter un permis à un conducteur
                    </h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    @using (Html.BeginForm("Ajouter", "ConducteurPermis", new { area = "Affectation" }, FormMethod.Post, new { @class = "form-horizontal", @role = "form", @id = "form" }))
                    {
                        <div class="form-horizontal">
                            <div class="form-group">
                                <input type="hidden" id="IdConducteurPermis" name="IdConducteurPermis" placeholder="ID" class="form-control">
                            </div>
                            <div class="form-group">
                                <label class="col-xs-5 control-label">Nom & Prénoms de conducteur</label>
                                <div class="col-xs-5">
                                    <select name="IdConducteur" data-placeholder="Choisir un conducteur" class="chosen-select" tabindex="4">
                                        @{
                                            foreach (var cond in Model.conducteur)
                                            {
                                                <option value="@cond.IdConducteur">@cond.NomConducteur @cond.PrenomConducteur</option>
                                            }
                                        }
                                    </select>
                                </div>
                                <div class="col-xs-2">
                                    <a class="btn btn-white btn-bitbucket" href="/Affectation/Conducteurs/Index">
                                        <i class="fa fa-plus-square"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-xs-5 control-label">Type de permis</label>
                                <div class="col-xs-5">
                                    <select name="IdCategoriePermis" data-placeholder="Choisir un permis" class="chosen-select" tabindex="4">
                                        @{
                                            foreach (var catp in Model.categoriePermis)
                                            {
                                                <option value="@catp.IdCategoriePermis">@catp.LibelleCategoriePermis</option>
                                            }
                                        }
                                    </select>
                                </div>
                                <div class="col-xs-2">
                                    <a class="btn btn-white btn-bitbucket" href="/Affectation/CategoriePermis/Index">
                                        <i class="fa fa-plus-square"></i>
                                    </a>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-xs-5 control-label">Numéro de Permis</label>
                                <div class="col-xs-7">
                                    <input type="text" id="NumPermis" name="NumPermis" placeholder="Numéro de Permis" class="form-control">
                                    @Html.ValidationMessage("NumPermis", "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-xs-5 control-label">Date d'expiration</label>
                                <div class="col-xs-7">
                                    <input type="text" class="form-control" data-mask="99/99/9999" placeholder="" id="DateExpire" name="DateExpire">
                                    @Html.ValidationMessage("DateExpire", "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                @*<label class="col-sm-4 control-label">En Activité</label>*@
                                <div class="col-sm-8">
                                    <input type="hidden" id="Supprimer" name="Supprimer" class="js-switch" checked="" value="False" />
                                    @Html.ValidationMessage("Supprimer", "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-xs-7 col-xs-offset-5">
                                    <button class="btn btn-white" type="submit">Annuler</button>
                                    <button class="btn btn-primary" type="submit">Ajouter</button>
                                </div>
                            </div>
                        </div>
                                            }
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-offset-2 col-lg-8">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <div>
                        Liste des conducteurs avec leur permis
                    </div>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="col-lg-offset-7 col-lg-5">
                        <input type="text" class="form-control input-sm m-b-xs" id="filter"
                               placeholder="Rechercher">
                    </div>
                    <table class="footable table table-stripped" data-page-size="8" data-filter=#filter>
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayName("ID")
                                </th>
                                <th>
                                    @Html.DisplayName("Nom & Prénoms")
                                </th>
                                <th>
                                    @Html.DisplayName("Type de Permis")
                                </th>
                                <th class="text-right">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.conducteurPermis.OrderByDescending(x => x.IdConducteurPermis))
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.IdConducteurPermis)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Conducteur.NomConducteur) @Html.DisplayFor(modelItem => item.Conducteur.PrenomConducteur)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.CategoriePermis.LibelleCategoriePermis)
                                    </td>
                                    <td class="text-right">
                                        <div class="btn-group">
                                            <a class="btn btn-white btn-bitbucket" data-toggle="modal" data-target="#e-@Html.DisplayFor(modelItem => item.IdConducteurPermis)">
                                                Modifier
                                            </a>
                                            <a class="btn btn-white btn-bitbucket" data-toggle="modal" data-target="#dl-@Html.DisplayFor(modelItem => item.IdConducteurPermis)">
                                                Détail
                                            </a>
                                            @{
                                                if (@item.Supprimer == false)
                                                {
                                                    <a class="btn btn-white btn-bitbucket" href="/Affectation/ConducteurPermis/Etat/@item.IdConducteurPermis">
                                                        Supprimer
                                                    </a>
                                                }
                                                else
                                                {
                                                    <a class="btn btn-white btn-bitbucket" href="/Affectation/ConducteurPermis/Etat/@item.IdConducteurPermis">
                                                        Restaurer
                                                    </a>
                                                }
                                            }
                                        </div>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

