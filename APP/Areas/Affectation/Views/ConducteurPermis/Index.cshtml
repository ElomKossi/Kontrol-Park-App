            </div>
        </div>

        <div class="modal inmodal fade" id="de-@tmp.IdConducteurPermis" tabindex="@tmp.IdConducteurPermis" role="dialog" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content animated bounceInRight">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                        <h4 class="modal-title">Supprimer un conducteur</h4>
                    </div>
                    @Html.HiddenFor(model => model.conducteurPermis)
                    <div class="modal-body">
                        <h3>Supprimer?</h3>
                        <div>
                            <dl class="dl-horizontal">
                                <dt>
                                    @Html.DisplayName("Nom & Prénoms")
                                </dt>
                                <dd>
                                    @tmp.Conducteur.NomConducteur +" "+ @tmp.Conducteur.PrenomConducteur
                                </dd>
                                <dt>
                                    @Html.DisplayName("Type de Permis")
                                </dt>
                                <dd>
                                    @tmp.CategoriePermis.LibelleCategoriePermis
                                </dd>
                                @*<dt>
                                    @Html.DisplayName("Numéro de Permis")
                                </dt>
                                <dd>
                                    @tmp.NumPermis
                                </dd>
                                <dt>
                                    @Html.DisplayName("Date d'expiration")
                                </dt>
                                <dd>
                                    @tmp.DateExpire
                                </dd>*@
                            </dl>
